{
    "name": "revelphp/client",
    "description": "A PHP API client for Revel POS.",
    "license": "MIT",
    "authors": [
        {
            "name": "Aidan Casey",
            "email": "aidan.casey@anteris.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "RevelPHP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RevelPHP\\Tests\\": "tests/"
        }
    },
    "require": {
        "php-http/discovery": "^1.14",
        "illuminate/collections": "^9.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "larapack/dd": "^1.1",
        "guzzlehttp/guzzle": "^7.4",
        "aidan-casey/mock-client": "dev-master"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "coverage": "./vendor/bin/phpunit --coverage-html ./build/coverage"
    }
}
